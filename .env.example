# Okta Configuration
# Your Okta organization name (without .okta.com)
OKTA_ORG_NAME=yourorg

# Okta API token with read permissions for users and groups
# Create at: Admin Console → Security → API → Tokens
OKTA_API_TOKEN=your_okta_api_token_here

# Braintrust API Keys
# Create at: Braintrust Organization → Settings → API Keys
# Each key needs permissions for user, group, role, and ACL management

# Production organization
BRAINTRUST_PROD_API_KEY=your_braintrust_production_api_key_here

# Development organization (optional)
BRAINTRUST_DEV_API_KEY=your_braintrust_dev_api_key_here

# Staging organization (optional)
BRAINTRUST_STAGING_API_KEY=your_braintrust_staging_api_key_here

# Logging Configuration (optional)
# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO